package com.class14;

import java.util.Scanner;

public class Interviewquestions2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//how to find Finnacci numbers
		Scanner s=new Scanner(System.in);
		int a=0;  
		int b=1;
		int c;
		System.out.println("Enter number for Fibonanacci series");
		int n=s.nextInt();
		System.out.print(a + " "+b);
		for(int i=2;i<n;i++) {
			c=a+b;
			System.out.print(" "+c);
			a=b;
			b=c;
		}
		
		
	}

}
